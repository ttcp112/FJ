    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>NüWeb F&B | Access denied</title>

    <!-- Bootstrap -->
    @Styles.Render("~/vendors/bootstrap/dist/css/bootstrap.min.css")
    <!-- Font Awesome -->
    @Styles.Render("~/vendors/font-awesome/css/font-awesome.min.css")

    <!-- Custom Theme Style -->
    @Styles.Render("~/build/css/custom.min.css")
</head>

<body class="nav-md">
