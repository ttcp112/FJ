        <div class="main_container">
            <!-- page content -->
            <div class="col-md-12">
                <div class="col-middle">
                    <div class="text-center text-center">
                        @*<h1 class="error-number">403</h1>*@
                        <h2>Access denied</h2>
                        <p>
                            You are not authorized to perform this operation. Please contact the Super Admin for advice.
                        </p>
                        <p>
                            @Html.ActionLink("Back to previous page", null, null, null, new { href = Request.UrlReferrer })
                            @*Please [<a onclick="history.go(-1); return false;" href="#">click here</a>] to go to previous page.*@
                        </p>
                        @*<div class="mid_center">
                                <h3>Search</h3>
                                <form>
                                    <div class="col-xs-12 form-group pull-right top_search">
                                        <div class="input-group">
                                            <input type="text" class="form-control" placeholder="Search for...">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" type="button" >Go!</button>
                                            </span>
                                        </div>
                                    </div>
                                </form>
                            </div>*@
                    </div>
                </div>
            </div>
            <!-- /page content -->
        </div>
    </div>

    <!-- jQuery -->
    @Scripts.Render("~/vendors/jquery/dist/jquery.min.js")
    <!-- Bootstrap -->
    @Scripts.Render("~/vendors/bootstrap/dist/js/bootstrap.min.js")
    <!-- FastClick -->
    @Scripts.Render("~/vendors/fastclick/lib/fastclick.js")
    <!-- NProgress -->
    @Scripts.Render("~/vendors/nprogress/nprogress.js")

    <!-- Custom Theme Scripts -->
    @Scripts.Render("~/build/js/custom.min.js")
</body>
</html>



