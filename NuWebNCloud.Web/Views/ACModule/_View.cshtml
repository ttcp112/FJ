@using NuWebNCloud.Shared
@model NuWebNCloud.Shared.Models.AccessControl.ModuleModels
<div role="main" class="right_col">
    <div class="">
        <div class="page-title">
            <div class="title_left" style="padding: 10px 0px;">
                @Html.Partial("~/Views/Shared/_breadcrumb.cshtml",
                new Dictionary<string, string> { { "A", "Access Control" }, { "B", "Modules" }, { "C", "View Detail" } })
            </div>
            <div class="clearfix"></div>
            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12 x_panel">
                    <div class="x_content">
                        <div class="row text-right">
                            <ul class="header-buttons pull-right list-inline">
                                <li><a href="#" onclick="CloseDetail(); return false;" class="btn btn-danger" id="btn-delete"><i class="fa fa-times"></i>&nbsp;Close</a></li>
                            </ul>
                        </div>
                        <div class="row form-horizontal form-label-left">
                            <div class="row form-horizontal form-label-left" style="margin-bottom:15px;">
                                <div class="form-group">
                                    <label class="control-label col-md-2 col-sm-2 col-xs-12">Module Name: </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        @Model.Name
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-md-2 col-sm-2 col-xs-12">Controller: </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        @Model.Controller
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-md-2 col-sm-2 col-xs-12">Sequence: </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        @Model.IndexNum
                                    </div>
                                </div>
                                @if (!string.IsNullOrEmpty(Model.ParentName))
                                {
                                    <div class="form-group">
                                        <label class="control-label col-md-2 col-sm-2 col-xs-12">Parent Module: </label>
                                        <div class="col-md-6 col-sm-6 col-xs-12">
                                            @Model.ParentName
                                        </div>
                                    </div>
                                }
                                @*<div class="form-group">
                                    <label class="control-label col-md-2 col-sm-2 col-xs-12">Created Date: </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        @Model.CreatedDate
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-md-2 col-sm-2 col-xs-12">Created User: </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        @Model.CreatedUser
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-md-2 col-sm-2 col-xs-12">Last Modified: </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        @Model.ModifiedDate
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-md-2 col-sm-2 col-xs-12">Modified User: </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        @Model.ModifiedUser
                                    </div>
                                </div>*@
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>